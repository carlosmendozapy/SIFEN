                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="23"/>
                        <xs:fractionDigits value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dTotCom" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Total + comisión
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="21"/>
                        <xs:fractionDigits value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dComi" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Comisión
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="16"/>
                        <xs:fractionDigits value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dIVAComi" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        IVA de la comisión
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="14"/>
                        <xs:fractionDigits value="8"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgInfPed">
        <xs:annotation>
            <xs:documentation>
                Informacion del pedido comercial relacionado a la factura
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dConPed" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Control de pedido de compra
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dInfInt" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Informacion de interes del emisor con respecto al pedido de compra
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="0"/>
                        <xs:maxLength value="500"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tDE">
        <xs:annotation>
            <xs:documentation>
                Datos del documento electronico
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dDVId" minOccurs="1" type="tDVer"/>
            <xs:element name="gOpeDE" type="tgCOpeDE"/>
            <xs:element name="gDTim" type="tgDTim"/>
            <xs:element name="gDatGralOpe" type="tgDaGOC"/>
            <xs:element name="gDtipDe" type="tgDtipDe"/>
            <xs:element minOccurs="0" name="gTotSub" type="tgTotSub"/>
            <xs:element minOccurs="0" name="gCamGen" type="tgCamGen"/>
            <xs:element minOccurs="0" maxOccurs="99" name="gCamDEAsoc" type="tgCamDEAsoc"/>   
            <xs:element name="dFecFirma" type="fecUTC"/>         
        </xs:sequence>
        <!--CDC pasa a ser atributo del rDE-->
        <xs:attribute name="Id" type="tCDC" use="required"/>
    </xs:complexType>

    <!-- Campos que identifican al documento asociado -->
    <xs:complexType name="tgCamDEAsoc">
        <xs:annotation>
            <xs:documentation>
                Cmapos que identifican al DE asociado
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipDocAso" type="tiTipDocAso"/>
            <xs:element name="dDesTipDocAso" type="tdDesTipDocAso"/>
            <xs:element name="dCdCDERef" type="tCDC" minOccurs="0"/>
            <xs:element name="dNumgTimDI" type="tdNumTim" minOccurs="0"/>
            <xs:element name="dEstDocAso" type="tdEst" minOccurs="0"/>
            <xs:element name="dPexpDocAso" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Punto de expedicion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumDocAso" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero del documento
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="7"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="iTipoDocAso" type="tiTIpoDoc" minOccurs="0"/>
            <xs:element name="dDTipoDocAso" type="tdDesTIpoDoc" minOccurs="0"/>
            <xs:element name="dFecDocImp" type="tdFeIniS" minOccurs="0"/>
            <xs:element name="dNumComRet" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de comprobante de retencion
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumResCF" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de resolucion de Credito Fiscal
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Campos de uso general -->
    <xs:complexType name="tgCamGen">
        <xs:annotation>
            <xs:documentation>
                Campos de uso general
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dOrdCompra" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de orden de compra
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dOrdVta" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de orden de venta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dAsiento" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de asiento contable
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="5"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgCamFuFD">
        <xs:annotation>
            <xs:documentation>
                Campos fuera de la firma digital
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dCarQR">
                <xs:annotation>
                    <xs:documentation>
                        Caracteres correspondiente al codigo QR
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="100"/>
                        <xs:maxLength value="600"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dInfAdic" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Informacion adicional de interes para el emisor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="5000"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="rDE">
        <xs:sequence>
            <xs:element name="dVerfor" type="tVerFor"/>
            <xs:element name="DE" type="tDE"/>
            <xs:element ref="ds:Signature">
                <xs:annotation>
                    <xs:documentation>
                        Firma Digital del DE
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="gCamFuFD" type="tgCamFuFD" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
