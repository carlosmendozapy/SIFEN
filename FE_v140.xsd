<?xml version="1.0" encoding="utf-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
        elementFormDefault="qualified"
        xmlns="http://www.sifen.gov.py/si"
        targetNamespace="http://www.sifen.gov.py/si">

    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#"
               schemaLocation="xmldsig-core-schema.xsd"/>
    <xs:include schemaLocation="Paises_v100.xsd"/>
    <xs:include schemaLocation="Monedas_v100.xsd"/>
    <xs:include schemaLocation="FE_Types_v140.xsd"/>

    <xs:complexType name="tgCOpeDE">
        <xs:annotation>
            <xs:documentation>
                Campos de la operacion del Documento Electronico
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipEmi" type="tiTipEmi"/>
            <xs:element name="dDesTipEmi" type="tdDesTipEmi"/>
            <xs:element name="dCodSeg" type="tiCodSe"/>
            <xs:element name="dInfoEmi" minOccurs="0" type="tdInfoEmi"/>
            <xs:element name="dInfoFisc" minOccurs="0" type="tdInfoFisc"/>
            <!-- <xs:element name="iIndPres" type="tiIndPres" minOccurs="0"/>
            <xs:element name="dDesIndPres" type="tdDesIndPres" minOccurs="0"/>
            <xs:element name="iTipTra" type="tiTipTra"/>
            <xs:element name="dDesTiTran" type="tdDesTiTran"/> -->
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgDTim">
        <xs:annotation>
            <xs:documentation>
                Campos de datos del timbrado
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTiDE" type="tiTiDE"/>
            <xs:element name="dDesTiDE" type="tdDesTiDE"/>
            <xs:element name="dNumTim" type="tdNumTim"/>
            <xs:element name="dEst" type="tdEst"/>
            <xs:element name="dPunExp" type="tdPunExp"/>
            <xs:element name="dNumDoc" type="tdNumDoc"/>
            <xs:element name="dFeIniT" type="tdFeIniT"/>
            <xs:element name="dFeFinT" type="tdFeFinT"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgEmis">
        <xs:annotation>
            <xs:documentation>
                Datos que identifican al emisor
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dRucEm" type="tRuc"/>
            <xs:element name="dDVEmi" type="tDVer"/>
            <xs:element name="iTipCont" type="tiTipCont"/>
            <xs:element name="cTipReg" minOccurs="0" type="tcTipReg"/>
            <xs:element name="dNomEmi">
                <xs:annotation>
                    <xs:documentation>
                        Nombre o razon social del emisor del DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:whiteSpace value="preserve"/>
                        <xs:minLength value="4"/>
                        <xs:maxLength value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNomFanEmi" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nombre o razon social del emisor del DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:whiteSpace value="preserve"/>
                        <xs:minLength value="4"/>
                        <xs:maxLength value="60"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDirEmi">
                <xs:annotation>
                    <xs:documentation>
                        Direccion del local donde se emite el DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:minLength value="1"/>
                        <xs:whiteSpace value="preserve"/>
                        <xs:maxLength value="150"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumCas">
                <xs:annotation>
                    <xs:documentation>
                        Numero de casa
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:totalDigits value="6"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCompDir1" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Complemento de direccion 1
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="150"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCompDir2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Complemento de direccion 2
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="15"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cCiuEmi" type="tcCiuEmi"/>
            <xs:element name="dDesCiuEmi" type="tdDesCiuEmi"/>
            <xs:element name="cDisEmi" minOccurs="0" type="tcDisEmi"/>
            <xs:element name="dDesDisEmi" minOccurs="0" type="tdDesDisEmi"/>
            <xs:element name="cDepEmi" type="tDepartamentos"/>
            <xs:element name="dDesDepEmi" type="tDesDepartamento"/>
            <xs:element name="dTelEmi" type="tdTel"/>
            <xs:element name="dEmailE" type="tEmail"/>
            <xs:element name="dDenSuc" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Denominacion interna del emisor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="30"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cActEco" type="tcActEco"/>
            <xs:element name="dDesActEco" type="tdDesActEco"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="tgDatRec">
        <xs:sequence>
            <xs:element name="iNatRec" type="tiNatRec"/>
            <xs:element name="iTiOpe" type="tiTiOpe"/>
            <xs:element name="cPaisRec" type="paisType"/>
            <xs:element name="dDesPaisRe" type="tDesPais"/>
            <xs:element name="iTiContRec" minOccurs="0" type="tiTipCont"/>
            <xs:element name="dRucRec" minOccurs="0" type="tRuc"/>
            <xs:element name="dDVRec" minOccurs="0" type="tDVer"/>
            <xs:element name="iTipIDRec" minOccurs="0" type="tiTipDoc"/>
            <xs:element name="dDTipIDRec" minOccurs="0" type="tdDtipDoc"/>
            <xs:element name="dNumIDRec" minOccurs="0" type="tdNumDocId"/>
            <xs:element name="dNomRec">
                <xs:annotation>
                    <xs:documentation>
                        Nombre o razon social del receptor del DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="60"/>
                        <xs:minLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNomFanRec" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Nombre o razon social del receptor del DE
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="60"/>
                        <xs:minLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDirRec" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Direccion del Receptor
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="noEmptyString">
                        <xs:maxLength value="150"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="dTelRec" minOccurs="0" type="tdTel"/>
            <xs:element name="dCelRec" minOccurs="0" type="tdTel"/>
            <xs:element name="dEmailRec" minOccurs="0" type="tEmail"/>
            <xs:element name="dCodCliente" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Codigo del Cliente
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:whiteSpace value="collapse"/>
                        <xs:maxLength value="5"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

<!--     <xs:complexType name="tgCamOC">
        <xs:annotation>
            <xs:documentation>
                Campos inherentes a la operacion comercial
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="gDatGralOpe" type="tgDaGOC"/>
            <xs:element name="gEmis" type="tgEmis"/>
            <xs:element name="gDatRec" type="tgDatRec"/>
        </xs:sequence>
    </xs:complexType> -->

    <xs:complexType name="tgDaGOC">
        <xs:annotation>
            <xs:documentation>
                Datos generales de la Operacion Comercial
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dFeEmiDE" type="fecUTC"/>
            <xs:element name="gOpeCom" type="tgOpeCom"/>
            <xs:element name="gEmis" type="tgEmis"/>
            <xs:element name="gDatRec" type="tgDatRec"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="tgOpeCom">
        <xs:annotation>
            <xs:documentation>
                Campos inherentes a la operacion comercial
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipTra" type="tiTipTra"/>
            <xs:element name="dDesTiTran" type="tdDesTiTran"/>
            <xs:element name="iTImp" type="tiTImp"/>
            <xs:element name="dDesTImp" type="tdDesTImp"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen el pago de la operacion con tarjeta de credito/debito-->
    <xs:complexType name="tgPagTarCD">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el pago de la operacion con tarjeta de credito/debito
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iDenTarj" type="tiDenTarj"/>
            <xs:element name="dDesDenTarj" type="tdDesDenTarj"/>
            <xs:element name="dRUCProTar" minOccurs="0" type="tRuc"/>
            <xs:element name="dDVProTar" minOccurs="0" type="tDVer"/>
            <xs:element name="iForProPa" type="tiForProPa" minOccurs="0"/>
            <xs:element name="dNunAuOpe" minOccurs="0" type="tdNunApOpe"/>
            <xs:element name="dNomTit">
                <xs:annotation>
                    <xs:documentation>
                        Nombre del titular de la tarjeta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="4"/>
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dNumTarj">
                <xs:annotation>
                    <xs:documentation>
                        Numero de la tarjeta
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgForPagCon">
        <xs:annotation>
            <xs:documentation>
                Campos que describen la forma de pago de la operacion al contado
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTiPago" type="tiTiPago"/>
            <xs:element name="dDesTiPag" type="tdDesTiPag"/>
            <xs:element name="dMonTiPag" type="tdMonTiPag"/>
            <xs:element name="gPagTarCD" minOccurs="0" type="tgPagTarCD"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen el pago de la operacion con cheque-->
    <xs:complexType name="tgPagCheq">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el pago de la operacion con cheque
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNumCheq" type="tNumCheq"/>
            <xs:element name="dBcoEmi">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="4"/>
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen las cuotas-->
    <xs:complexType name="tgCuotas">
        <xs:annotation>
            <xs:documentation>
                Campos que describen las cuotas
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            
            <xs:element name="dMonCuota" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Monto de cada cuota
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="14"/>
                        <xs:fractionDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dVencCuo" minOccurs="0" type="tFecAAAAMMDDguion"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgOpeCred">
        <xs:annotation>
            <xs:documentation>
                Campos que describen la operacion a credito
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNcuotas">
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dMonEntlni" type="tMontoBase"/>
            <xs:element name="gCuotas" minOccurs="0" maxOccurs="999" type="tgCuotas"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen las informaciones de compras publicas-->
    <xs:complexType name="tgCompPub">
        <xs:annotation>
            <xs:documentation>
                Campos que describen informaciones de compras publicas
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dModCont" type="tdModCont"/>
            <xs:element name="dEntCont " type="tdEntCont"/>
            <xs:element name="dAnoCont" type="tdAnoCont"/>
            <xs:element name="dSecCont" type="tdSecCont"/>
            <xs:element name="dFeCodCont" type="tFecAAAAMMDDguion"/>
        </xs:sequence>
    </xs:complexType>
   
    <xs:complexType name="tgInfTransp">
        <!--TODO: Realizar la codificacion de esto-->
        <xs:annotation>
            <xs:documentation>
                Campos que describen las informaciones del transporte
            </xs:documentation>
        </xs:annotation>
    </xs:complexType>
          
    <!--Campos que componen la factura electronica-->
    <xs:complexType name="tgCamFE">
        <xs:annotation>
            <xs:documentation>
                Campos que componen la factura electronica
            </xs:documentation>
        </xs:annotation>        
        <xs:sequence>
        	<xs:element name="iIndPres" type="tiIndPres" minOccurs="0"/>
        	 <xs:element name="dDesIndPres" type="tdDesIndPres" minOccurs="0"/>
            <xs:element name="gCompPub" minOccurs="0" type="tgCompPub"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="tgDatMon">
        <xs:annotation>
            <xs:documentation>
                Datos de la moneda
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iMonOpe" type="cMondT"/>
            <xs:element name="dDmonOpe" type="xs:NCName"/>
            <xs:element name="dTipCam" type="tdTipCam"/>
        </xs:sequence>
    </xs:complexType>

    <!--Campos de Precio descuentos por item-->
    <xs:complexType name="tgValorItem">
        <xs:annotation>
            <xs:documentation>
                Campos que describen los precios, descuentos y valor total por item
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dPUniProSer">
                <xs:annotation>
                    <xs:documentation>
                        Precio unitario del producto y/o servicio
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="16"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="99999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dDescItem">
                <xs:annotation>
                    <xs:documentation>
                        Descuento sobre el precio unitario por ítem
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="14"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPorcDesIt">
                <xs:annotation>
                    <xs:documentation>
                        Porcentaje de descuento por item
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="3"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dTotOpeItem">
                <xs:annotation>
                    <xs:documentation>
                        Valor total de la operacion por item
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="18"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="9999999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Campos que describen el IVA de la operacion-->
    <xs:complexType name="tgCamIVA">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el IVA de la operación por ítem
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iAfecIVA" type="tiAfecIVA"/>
            <xs:element name="dDesAfecIVA" type="tdDesAfecIVA"/>
            <xs:element name="dPropIVA" type="tdPropIVA"/>
            <xs:element name="dTasaIVA" type="tdTasaIVA"/>
            <xs:element name="dBasGravIVA">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Base gravada del IVA por ítem
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="18"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="9999999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dLiqIVAItem">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Liquidacion del IVA por ítem
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="17"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="999999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Campos que describen el ISC de la operacion -->
    <xs:complexType name="tgCamISC">
        <xs:annotation>
            <xs:documentation>
                Campos que describen el ISC de la operacion por ítem
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="cCatISC" type="tcCatISC"/>
            <xs:element name="dDesCatISC" type="tdDesCatISC"/>
            <xs:element name="cTasaISC" type="tcTasaISC"/>
            <xs:element name="dBaseGravISC">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Base gravada del ISC por item
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="18"/>
                        <xs:fractionDigits value="8"/>
			            <xs:maxInclusive value="9999999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dLiqISCItem">
                <xs:simpleType>
                    <xs:annotation>
                        <xs:documentation>
                            Liquidacion del ISC por item
                        </xs:documentation>
                    </xs:annotation>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="17"/>
                        <xs:fractionDigits value="8"/>
                        <xs:maxInclusive value="999999999.99999999"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Grupo de rastreo de la mercaderia-->
    <xs:complexType name="tgRasMerc">
        <xs:annotation>
            <xs:documentation>
                Datos de rastreo de la mercaderia
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="dNumLote" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="80"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dVencMerc" minOccurs="0" type="tdFeIniS"/>
            <xs:element name="dNSerie" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Numero de serie
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="9"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!--Sector Automovilistico Vehiculos nuevos-->
    <xs:complexType name="tgVehNuevo">
        <xs:annotation>
            <xs:documentation>
                Detalle de vehiculos nuevos
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="iTipOpVN" type="tiTipOpVN" minOccurs="0"/>
            <xs:element name="dDesTipOpVN" type="tdDesTipOpVN" minOccurs="0"/>
            <xs:element name="dChasis" type="tdChasis" minOccurs="0"/>
            <xs:element name="dColor" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPotencia" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Potencia del motor (CV)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dCapMot" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Capacidad del motor (cc)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:positiveInteger">
                        <xs:totalDigits value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="dPNet" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Peso Neto (Toneladas)
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:decimal">
                        <xs:totalDigits value="10"/>
